<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.my.count.dao.IHomeMapper">
<select id="labels" resultType="com.my.count.model.HomeDto">
    select * from home;
</select>
    <insert id="add" parameterType="com.my.count.model.HomeDto">
        insert into home(label,custom,`count`,comment,`type`,cusDate) values(#{label},#{custom},#{count},#{comment},#{type},#{cusDate})
    </insert>
</mapper>